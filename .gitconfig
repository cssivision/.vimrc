[user]
	email = cssivision@gmail.com
	name = cssivision
[credential]
	helper = store

[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true
    features = villsau

[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default

[include]
	path = ~/.themes.gitconfig
